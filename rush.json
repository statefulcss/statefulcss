{
	"$schema": "https://developer.microsoft.com/json-schemas/rush/v5/rush.schema.json",
	"rushVersion": "5.166.0",
	"pnpmVersion": "10.28.2",
	"nodeSupportedVersionRange": ">=24.11.1 <25.0.0",
	"ensureConsistentVersions": true,
	"projectFolderMaxDepth": 2,
	"projectFolderMinDepth": 2,
	"gitPolicy": {
		"versionBumpCommitMessage": "[repo] Bump versions [skip ci]",
		"changeLogUpdateCommitMessage": "[repo] Update changelogs [skip ci]",
		"changefilesCommitMessage": "[repo] Rush change"
	},
	"repository": {
		"url": "https://github.com/statefulcss/statefulcss",
		"defaultRemote": "origin"
	},
	"eventHooks": {
		"preRushInstall": [],
		"postRushInstall": [],
		"preRushBuild": [],
		"postRushBuild": [],
		"preRushx": [],
		"postRushx": []
	},
	"telemetryEnabled": false,
	"allowedProjectTags": ["config"],
	"projects": []
}
